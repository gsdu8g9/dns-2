{
    "name": "icicleio/dns",
    "description": "Asynchronous DNS resolver for Icicle.",
    "keywords": [
        "dns",
        "resolver",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "0.2 - 0.4",
        "daverandom/libdns": "~1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "mockery/mockery": "0.9.*",
        "satooshi/php-coveralls": "dev-master",
        "symfony/yaml": "~2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev",
            "dev-development": "0.2-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Icicle\\Dns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\": "vendor/icicleio/icicle/tests",
            "Icicle\\Tests\\Dns\\": "tests"
        }
    }
}
