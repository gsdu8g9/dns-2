{
    "name": "icicleio/dns",
    "description": "Asynchronous DNS for Icicle.",
    "keywords": [
        "dns",
        "resolver",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "~2.0",
        "icicleio/socket": "~2.0",
        "daverandom/libdns": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~1.0",
        "symfony/yaml": "^2.7"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Icicle\\Dns\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Tests\\Dns\\": "tests"
        }
    }
}
